<main>
    <h1>Music Rights</h1>
    <table class="table-list">
        <tr>
            <th>
                Name of the song
            </th>
            <th>
                Artist
            </th>
            <th>Last Sold Price</th>
            <th>
                Current Price
            </th>
        </tr>
        {{#each songs}}
            <tr onclick="window.location='/{{_id}}'" class="clickable-row">
                <td>
                    {{name}}
                </td>
                <td>
                    {{artist}}
                </td>
                <td>
                    {{lastSoldPrice}}
                </td>
                <td>
                    {{price}}
                </td>
            </tr>
        {{/each}}
    </table>
</main>