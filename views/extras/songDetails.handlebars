<main>
    <h1>{{songs.name}}</h1>
    <div class="artist-name">
        <h2>Artist</h2>
        <h3>{{songs.artist}}</h3>
    </div>
    <div class="price">
        <h2>Price</h2>
        <h3>${{songs.price}}</h3>
    </div>
    <div class="genre">
        <h2>Genre</h2>
        <h3>{{songs.genre}}</h3>
    </div>
    <div class="description-song">
        <h2>Description</h2>
        <h3>{{songs.description}}</h3>
    </div>
    <div class="lastSoldPrice">
        <h2>Last Sold Price</h2>
        <h3>${{songs.lastSoldPrice}}</h3>
    </div>
    <div class="lastSoldPrice">
        <h2>Number of Tracks</h2>
        <h3>{{songs.numberOfTracks}}</h3>
    </div>
    <div class="lastSoldPrice">
        <h2>currently Available</h2>
        <h3>{{songs.currentlyAvailable}}</h3>
    </div>
    <form id="buy-songs" action="/wallet" method="POST" class="buy-songs">
        <button type="submit">Buy Now</button>
</main>